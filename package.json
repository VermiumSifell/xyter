    "Joshua Schmitt <me@jqshuv.xyz> (https://jqshuv.xyz)"
  ],
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/ZynerOrg/xyter/issues",
    "email": "vermium@zyner.org"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "@types/i18next-fs-backend": "^1.1.2",
    "axios": "^1.0.0",
    "chance": "^1.1.8",
    "date-fns": "^2.29.3",
    "discord.js": "^14.0.0",
    "dotenv": "^16.0.1",
    "node-schedule": "^2.1.0",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.6.1"
