{
  "name": "xyter",
  "version": "3.1.0",
  "description": "Earn credits while chatting! And more",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsc --watch & NODE_ENV=development nodemon dist",
    "build": "tsc -p .",
    "prisma:generate": "prisma generate",
